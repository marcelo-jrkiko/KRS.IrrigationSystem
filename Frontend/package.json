        "crypto-js": "^4.1.1",
        "file-saver": "^2.0.5",
        "maska": "^2.1.11",
        "moment": "^2.29.4",
        "ol": "^7.3.0",
        "pinia": "^2.0.28",
        "pinia-plugin-persistedstate": "^3.0.2",
        "rollup-plugin-visualizer": "^5.9.0",
        "sweetalert2": "^11.7.5",
        "tippy.js": "^6.3.7",
        "typescript-cookie": "^1.0.6",
        "unplugin-vue-components": "^0.24.1",
        "vue": "^3.3.0",
        "vue-chartjs": "^5.3.0",
        "vue-debounce": "^4.0.0",
        "vue-i18n": "^9.2.2",
